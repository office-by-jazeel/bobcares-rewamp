// This is your Prisma schema file,
// learn more about everything in a schema: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Newsletter {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  ip        String?
  createdAt DateTime @default(now())

  @@map("newsletter")
}

